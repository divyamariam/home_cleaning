<h1>Employee Management</h1>
<br>
<table><thead><tr><th><b><%= link_to 'Add New Employee', new_employee_path %></b>
</th><th></th><th><%= link_to 'Back to Home', '/members/view' %></th><th></th><th><strong><%= link_to 'Logout', '/members/logout' %></strong></th></tr></thead></table>
<br>
<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
      <th></th><th></th>
    </tr>
  </thead>

  <tbody>
    <% @employees.each do |employee| %>
      <tr>
        <td><%= employee.username %></td><td></td>
        <td><%= link_to 'View', employee %></td>
        <td><%= link_to 'Edit', edit_employee_path(employee) %></td>
        <td><%= link_to 'Delete', employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


